
<img src="{{ avatar_url }}" class ="thumbnail img-responsive" alt="Foto">
<h1>{{ name }}</h1>
<h2>{{ login }}</h2>

<hr/>

<ul class="list-unstyled">
    {%  for detail in details %}
        <li>{{ detail }}</li>
    {% endfor %}
    <li><a href="http://www.google.com">{{ blog }}</a></li>

</ul>
<hr>

<ul class ="list-inline">

    {% for key, value in social_data %}
        <li><span class="social_metric_count">{{ value }}</span>
            <br>
            <span class ="small"> {{ key }}</span>
        </li>
    {% endfor %}

</ul>
